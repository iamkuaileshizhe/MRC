<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:stv="http://schemas.android.com/apk/res-auto"
    android:id="@+id/lianxiren_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/divider"
    android:orientation="vertical">


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_0.5dp"
        android:background="@color/divider" />

    <com.neocom.mobilerefueling.view.isEditeTextView
        android:id="@+id/lianxiren_phone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_normal_hight"
        android:layout_below="@+id/lianxiren_mobi"
        stv:isEdit="true"
        stv:mHint="请输入联系电话"
        stv:mText=""
        stv:mTitle="联系电话" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_0.5dp"
        android:background="@color/divider" />

    <com.neocom.mobilerefueling.view.isEditeTextView
        android:id="@+id/lianxiren_email"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_normal_hight"
        android:layout_below="@+id/lianxiren_phone"
        stv:isEdit="true"
        stv:mHint="请输入电子邮箱"
        stv:mText=""
        stv:mTitle="电子邮箱" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_0.5dp"
        android:background="@color/divider" />

    <com.neocom.mobilerefueling.view.isEditeTextView
        android:id="@+id/lianxiren_fox"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_normal_hight"
        android:layout_below="@+id/lianxiren_email"
        android:visibility="gone"
        stv:isEdit="true"
        stv:mHint="请输入传真号码"
        stv:mText=""
        stv:mTitle="传真号码" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_0.5dp"
        android:background="@color/divider" />

    <com.neocom.mobilerefueling.view.isEditeTextView
        android:id="@+id/lianxiren_qq"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_normal_hight"
        android:layout_below="@+id/lianxiren_fox"
        stv:isEdit="true"
        stv:mHint="请输入QQ号码"
        stv:mText=""
        stv:mTitle="QQ号码" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_0.5dp"
        android:background="@color/divider" />

    <com.neocom.mobilerefueling.view.isEditeTextView
        android:id="@+id/lianxiren_wx"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_normal_hight"
        android:layout_below="@+id/lianxiren_qq"
        stv:isEdit="true"
        stv:mHint="请输入微信号码"
        stv:mText=""
        stv:mTitle="微信号码" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_0.5dp"
        android:background="@color/divider" />

    <!--<com.neocom.mobilerefueling.view.isEditeTextView-->
