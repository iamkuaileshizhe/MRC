<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/divider_line"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/top_title_bar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/abc_action_bar_default_height_material"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@color/white"
        android:gravity="center"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/top_bar_finish_ll"
            android:layout_width="@dimen/_20dp"
            android:layout_height="30dp"
            android:layout_marginLeft="15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:src="@mipmap/back_top" />

        </LinearLayout>


        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/top_bar_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@null"
                android:cursorVisible="false"
                android:text="补给详情"
                android:textColor="@android:color/black"
                android:textColorHint="@color/colorBarText"
                android:textSize="@dimen/_20sp" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/top_bar_ok_ll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="7dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="7dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/addmore"
                android:visibility="invisible" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="确定"
                android:textColor="@color/colorBarText"
                android:textSize="17sp"
                android:visibility="invisible" />
        </LinearLayout>
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/bottom_ll"
        android:layout_below="@+id/top_title_bar"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/supply_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5dp"
               />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/supply_car_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/supply_car_driver"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/supply_car_telphone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/move_car_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/move_car_driver"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
               />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/move_car_telphone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/surplus_oil"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/supply_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/supply_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/supply_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@color/divider_line" />





            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:gravity="center_vertical">

                <com.neocom.mobilerefueling.view.OrderConbindView
                    android:id="@+id/batch_id"
                    android:layout_width="match_parent"
                    android:layout_toLeftOf="@+id/more"
                    android:layout_height="wrap_content"
                    />

                <ImageView
                    android:id="@+id/more"
                    android:layout_width="@dimen/_10dp"
                    android:layout_height="15dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/_10dp"
                    android:background="@mipmap/load_more" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />




            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:visibility="gone"
                android:gravity="center_vertical">

                <com.neocom.mobilerefueling.view.OrderConbindView
                    android:id="@+id/file_old_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@+id/more_a"
                    />


                <ImageView
                    android:id="@+id/more_a"
                    android:layout_width="@dimen/_10dp"
                    android:layout_height="15dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/_10dp"
                    android:background="@mipmap/load_more" />

            </RelativeLayout>




            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/record_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/confirm_people"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/confirm_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/c_dt"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />


            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/create_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                />


            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/u_dt"
                android:layout_width="match_parent"
                android:visibility="gone"
                android:layout_height="wrap_content"
                />


            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/modify_name"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:layout_marginTop="@dimen/_5dp"
                android:background="@color/divider_line" />

            <com.neocom.mobilerefueling.view.OrderConbindView
                android:id="@+id/remark_ifm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:background="@color/divider_line" />
        </LinearLayout>
    </ScrollView>


    <LinearLayout
        android:id="@+id/bottom_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@android:color/white"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="5dp"
        android:visibility="gone">


        <Button
            android:id="@+id/pay_cancle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:background="@drawable/bg_btn_login_unselected"
            android:text="不通过" />

        <Button
            android:id="@+id/pay_now"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:textColor="@android:color/white"
            android:textSize="@dimen/normal_font_size"
            android:background="@drawable/bg_btn_login_selected"
            android:text="通过" />
    </LinearLayout>


</RelativeLayout>