<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
    <shape >
        <solid android:color="@color/loading_tip_solid"/>
        <stroke android:color="@color/loading_tip_stroke" android:width="1.5dp"/>
        <corners android:radius="4dp"/>
    </shape>
    </item>
    <item android:state_pressed="true">
        <shape >
            <solid android:color="@color/loading_tip_solid_press"/>
            <corners android:radius="4dp"/>
            <stroke android:color="@color/loading_tip_stroke_press" android:width="1.5dp"/>
        </shape>
    </item>
</selector>